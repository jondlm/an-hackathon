{
  "manifest_version": 2,
  "name": "Translator",
  "description": "AppNexus hackathon translation extension",
  "version": "0.0.1",
  "permissions": [ "*://*/*" ],
  "content_scripts": [{
    "matches": ["<all_urls>"],
    "js": ["index.js"],
    "css": ["index.css"],
    "run_at": "document_end"
  }]
}
