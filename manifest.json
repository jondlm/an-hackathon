{
  "manifest_version": 2,
  "name": "Translator",
  "description": "AppNexus hackathon translation extension",
  "version": "0.0.1",
  "permissions": [ "*://*/*" ],
  "content_scripts": [{
    "matches": ["*://*.devnxs.net/*"],
    "js": ["jquery.js", "index.js"],
    "css": ["animate.css", "styles.css"],
    "run_at": "document_end"
  }],
 "web_accessible_resources": [
   "sidebar.html"
 ]
}
